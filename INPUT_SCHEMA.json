{
  "title": "CheerioCrawler Template",
  "description": "lorem ipsum",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "startUrls": {
      "title": "Start URLs",
      "type": "array",
      "description": "URLs to start with.",
      "editor": "json",
      "prefill": [
        {
          "url": "https://www.example.com/category/15",
          "userData": { "handlerType": "handleStart", "storeName": "exampleStore" }
        }
      ]
    },
    "proxyEndpoint": {
      "title": "Proxy Endpoint URL",
      "type": "string",
      "description": "Proxy Endpoint URL to get proxies from",
      "editor": "textfield",
      "prefill": "https://server.com/proxies",
      "pattern": "http[s]?://.*"
    }
  },
  "required": [
    "startUrls",
    "proxyEndpoint"
  ]
}
